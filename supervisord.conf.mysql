[supervisord]
nodaemon=true

[program:apache2]
command=/usr/sbin/httpd -DFOREGROUND
autostart=true
autorestart=true
stdout_logfile=/var/log/httpd/access.log
stderr_logfile=/var/log/httpd/error.log

[program:mysql]
command=/usr/bin/mysqld_safe
autostart=true
autorestart=true
stdout_logfile=/var/log/mysql/mysql.log
stderr_logfile=/var/log/mysql/mysql.err.log
